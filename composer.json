{
  "name": "beastbytes/yii-tracy-panel-view",
  "description": "View panel for Yii Tracy",
  "keywords": [
    "yii",
    "Tracy",
    "debugger",
    "view"
  ],
  "type": "library",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "BSD-3-Clause",
  "require": {
    "php": "^8.1",
    "psr/container": "2.0",
    "beastbytes/yii-tracy": "dev-main",
    "yiisoft/translator-message-php": "^1.0",
    "yiisoft/yii-view-renderer": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "vimeo/psalm": "^5.0",
    "yiisoft/di": "^1.0",
    "yiisoft/dummy-provider": "^1.0",
    "yiisoft/test-support": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "BeastBytes\\Yii\\Tracy\\Panel\\View\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BeastBytes\\Yii\\Tracy\\Panel\\View\\Tests\\": "tests/"
    }
  },
  "extra": {
    "config-plugin-options": {
      "source-directory": "config"
    },
    "config-plugin": {
      "events-web": "events-web.php",
      "params-web": "params-web.php"
    }
  }
}
